#include <stdio.h>
#include "maze.h"
#include "my.h"

void print_wall(maze_t *maze)
{
	int length = 0;

	for (int i = 0; i < maze->rows->nb_set; i++) {
		length = maze->rows->size_set[i].length;
		for (int j = 0; j < length; j++) {
			if (j < (length - 1))
				my_putchar('.');
			else
				my_putchar('#');
		}
	}
}
