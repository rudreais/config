#include <stdbool.h>

typedef struct {
	int x;
	int y;
} area_t;

typedef struct {
	int length;
} set_t;

typedef struct row {
	set_t *size_set;
	int nb_set;
} row_t;

typedef struct maze {
	area_t *size;
	row_t *rows;
	bool success;
} maze_t;


//	init.c
void init_maze(maze_t *maze, char **av);
void destroy_maze(maze_t *maze);

//	gen.c
void generate(row_t *rows, area_t *size);

//	debug.c
void debug(maze_t *maze);

//	wall/create_map.c
void print_wall(maze_t *maze);
